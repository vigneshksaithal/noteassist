#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/bun@1.1.27/node_modules/bun/bin/node_modules:/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/bun@1.1.27/node_modules/bun/node_modules:/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/bun@1.1.27/node_modules:/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/bun@1.1.27/node_modules/bun/bin/node_modules:/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/bun@1.1.27/node_modules/bun/node_modules:/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/bun@1.1.27/node_modules:/Users/vigneshaithal/Projects/LearnAssist/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/bun.exe"   "$@"
exit $?
